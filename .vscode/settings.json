{
	"ansible.python.interpreterPath": "/usr/bin/python3",
	"python.analysis.typeCheckingMode": "basic",
	"[python]": {
		"editor.defaultFormatter": "ms-python.autopep8"
	},
	"sqltools.connections": [
		{
			"mysqlOptions": {
				"authProtocol": "default",
				"enableSsl": "Disabled"
			},
			"previewLimit": 50,
			"server": "localhost",
			"port": 3306,
			"driver": "MariaDB",
			"name": "Biotrace",
			"database": "Biotrace",
			"username": "dev",
			"password": "dev"
		}
	],
	"djlint.useVenv": true,
	"djlint.pythonPath": "python3"
}